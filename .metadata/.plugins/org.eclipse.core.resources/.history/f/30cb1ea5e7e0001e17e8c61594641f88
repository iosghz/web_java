	 */
	
	public static void main(String[] args) {
		
		
		double pi = 3.14;
		
		Scanner sc = new Scanner(System.in);
		
		int num1 = 0, num2 = 0;
		
		
		System.out.println("첫번째 반지름을 입력하세요");
		
		sc.nextInt();
		
		System.out.println("두번째 반지름을 입력하세요");
		
		sc.nextInt();
		
		sd
		System.out.println(pi * num1 * num2);
		
		
	}
}
